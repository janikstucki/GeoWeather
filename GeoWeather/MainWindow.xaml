<Window x:Class="GeoWeather.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeoWeather"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="stationYCoordinateTBX" HorizontalAlignment="Left" Margin="374,239,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Background="AliceBlue" TextChanged="StationYCoordinateTBX_TextChanged"/>
        <TextBox x:Name="stationXCoordinateTBX" HorizontalAlignment="Left" Margin="374,0,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="120" Background="AliceBlue" TextChanged="StationXCoordinateTBX_TextChanged"/>
        <Button x:Name="SubmitBTN" Content="Button" HorizontalAlignment="Left" Margin="414,296,0,0" VerticalAlignment="Top" Click="SubmitBTN_Click"/>
        <TextBox x:Name="stationNameTBX" HorizontalAlignment="Left" Margin="374,174,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Background="AliceBlue" TextChanged="StationXCoordinateTBX_TextChanged"/>

    </Grid>
</Window>
