<Window x:Class="GeoWeather.StationOverview"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeoWeather"
        mc:Ignorable="d"
        Title="StationOverview" Height="450" Width="800">
    <Grid>
        <Label x:Name="StationLBL" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="stationsListBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="29,50,367,190" SelectionMode="Single"/>
    </Grid>
</Window>
