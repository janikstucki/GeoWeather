<Window x:Class="GeoWeather.overview"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeoWeather"
        mc:Ignorable="d"
        Title="overview" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="stationsListBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="162,94,234,146" SelectionChanged="stationsListBox_SelectionChanged" SelectionMode="Single"/>
        <Button x:Name="StationOverviewBTN" Content="Button" HorizontalAlignment="Left" Margin="42,32,0,0" VerticalAlignment="Top" Click="stationsListBox_SelectionChanged"/>
        <Button x:Name="testBTN" Content="Button" HorizontalAlignment="Left" Margin="651,42,0,0" VerticalAlignment="Top" Click="testBTN_Click"/>

    </Grid>
</Window>
